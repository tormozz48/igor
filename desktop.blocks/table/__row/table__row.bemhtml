block('table').elem('row')(

    def().match(!this.ctx.content)(function() {
        return false;
    }),

    tag()('tr'),

    content()(function() {
        var c = this.ctx.content;
        return Object.keys(c).map(function(key) {
            return {
               block: this.block,
               elem: 'col',
               content: c[key]
            }
        });
    })


)