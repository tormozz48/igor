block('menu').content()(

    tag()('ul'),

    content()(function() {
        return this.ctx.content.items.map(function(item, index) {
            return {
                elem: 'item',
                content: [
                    {
                        block: 'link',
                        url: '#'+index,
                        content: item.name
                    }
                ]
            };
        });
    })
)