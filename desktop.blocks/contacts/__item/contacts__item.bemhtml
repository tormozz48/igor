block('contacts').elem('item')(

	def().match(!this.ctx.content)(function() {
		return false;
	}),

	content()(function() {
	    var c = this.ctx.content;
		return [
            {
                block: this.block,
                elem: 'label',
                content: c.label
            },
            {
                block: this.block,
                elem: 'value',
                content: c.value
            }
		]
	})
)